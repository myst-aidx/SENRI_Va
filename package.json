{
  "name": "fortune-telling-app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:frontend": "vite",
    "dev:backend": "cd server && npm run dev",
    "dev:all": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "start:all": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build": "vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 999",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headless": "cypress run --headless",
    "test:e2e": "cypress open",
    "test:e2e:headless": "start-server-and-test dev http://localhost:5173 cypress:run:headless",
    "test:e2e:ci": "cypress run --headless --browser chrome",
    "test:selenium": "cross-env TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} mocha -r ts-node/register tests/selenium/**/*.test.ts --timeout 60000",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --watch",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "check-db": "node scripts/checkMongoConnection.js",
    "create-users": "cross-env NODE_ENV=development ts-node --files scripts/createTestUser.ts",
    "clean": "if exist node_modules rmdir /s /q node_modules && if exist package-lock.json del package-lock.json",
    "postinstall": "npm run build",
    "create-admin": "ts-node server/src/scripts/createInitialAdmin.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@google/generative-ai": "^0.21.0",
    "@mui/material": "^5.15.10",
    "@mui/styles": "^5.15.10",
    "@types/axios": "^0.9.36",
    "@types/chart.js": "^2.9.41",
    "@types/express": "^4.17.21",
    "@types/jspdf": "^1.3.3",
    "@types/react-toastify": "^4.0.2",
    "@types/recharts": "^1.8.29",
    "@types/winston": "^2.4.4",
    "astronomy-bundle": "^7.7.7",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.4.7",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "framer-motion": "^11.0.5",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^2.5.2",
    "jspdf-autotable": "^3.8.4",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.344.0",
    "openai": "^4.77.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.22.1",
    "react-toastify": "^11.0.3",
    "recharts": "^2.15.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@babel/plugin-transform-runtime": "^7.26.8",
    "@babel/preset-env": "^7.26.8",
    "@babel/preset-typescript": "^7.26.0",
    "@cypress/react": "^9.0.0",
    "@cypress/vite-dev-server": "^5.0.5",
    "@eslint/js": "^8.56.0",
    "@jest/globals": "^29.7.0",
    "@swc-node/register": "^1.10.9",
    "@swc/core": "^1.10.7",
    "@swc/register": "^0.1.10",
    "@types/bcrypt": "^5.0.2",
    "@types/chai": "^4.3.11",
    "@types/chromedriver": "^81.0.5",
    "@types/cors": "^2.8.17",
    "@types/cypress": "^0.1.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mocha": "^10.0.10",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^20.17.18",
    "@types/react-dom": "^19.0.3",
    "@types/selenium-webdriver": "^4.1.28",
    "@types/sinon": "^17.0.3",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "babel-jest": "^29.7.0",
    "chai": "^4.3.10",
    "chromedriver": "^132.0.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "cypress": "^13.17.0",
    "dotenv": "^16.4.7",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "mongodb-memory-server": "^10.1.3",
    "mongoose": "^8.10.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.35",
    "prettier": "^3.1.1",
    "selenium-webdriver": "^4.27.0",
    "start-server-and-test": "^2.0.10",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.5",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.8"
  },
  "ts-node": {
    "transpileOnly": true,
    "files": true,
    "compilerOptions": {
      "module": "commonjs",
      "resolveJsonModule": true,
      "esModuleInterop": true
    }
  }
}
